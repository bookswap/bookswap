
<div id="have_block">
<div id="haves">
<% @user.haves.each do |have|%>
<p><%= have %></p>
<% end %>
</div>
<%= form_for(@have) do |f| %>
<input name="[title]">
<%= f.submit "Submit!"%>
<% end %>
</div>

<div id="want_block">

<div id="wants">
<% @user.wants.each do |want| %>
<p><%= want %></p>
<% end %>
</div>
<%= form_for(@want)%>
<input name="[title]">
<%= f.submit "Submit!"%>
<% end %>
</div>